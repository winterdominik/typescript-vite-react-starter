{
  "name": "typescript-vite-react-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "ts-node $npm_package_custom_mockServer/server.ts",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.tsx,json '**/*.{ts,tsx,json}'",
    "lint:fix": "eslint --fix --ext .ts,.tsx,json '**/*.{ts,tsx,json}'",
    "format": "prettier --write '**/*.{ts,tsx,json}'",
    "stylelint-check": "stylelint-config-prettier-check",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "stylelint --fix '**/*.css'"
  },
  "dependencies": {
    "antd": "^4.22.2",
    "eslint-config-prettier": "^8.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-stylelint": "^3.0.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{ts,tsx,json}": "eslint --fix",
    "**/*.css": "stylelint-check --fix"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "custom": {
    "mockServer": "mockServer"
  }
}
