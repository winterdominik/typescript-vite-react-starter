{
  "name": "typescript-vite-react-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "custom": {
    "mockServer": "mockServer"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "mockServer": "json-server --config $npm_package_custom_mockServer/config.json --middlewares $npm_package_custom_mockServer/server.js --watch $npm_package_custom_mockServer/db.json ",
    "server": "ts-node $npm_package_custom_mockServer/server.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "json-server": "^0.17.0",
    "msw": "^0.44.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}